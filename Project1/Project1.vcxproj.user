<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ShowAllFiles>true</ShowAllFiles>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommandArguments>$(SolutionDir)shakespeare $(SolutionDir)temp $(SolutionDir)out $(SolutionDir)$(Platform)\$(Configuration)\MapDll.dll $(SolutionDir)$(Platform)\$(Configuration)\ReduceDll.dll</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerEnvironment>$(SolutionDir)MapDll;$(SolutionDir)ReduceDll;$(SolutionDir)FileManagerDll;$(Path)</LocalDebuggerEnvironment>
  </PropertyGroup>
</Project>